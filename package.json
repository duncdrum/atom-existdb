{
  "name": "existdb",
  "main": "./lib/existdb",
  "version": "0.0.1",
  "description": "Linting and autocomplete for XQuery scripts written for the eXist-db Native XML Database.",
  "keywords": [
      "xmldb",
      "eXist-db",
      "xquery",
      "xml"
  ],
  "activationHooks": ["language-jsoniq:grammar-used"],
  "repository": "https://github.com/wolfgangmm/atom-existdb",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
      "atom-linter": "^4.3.1",
      "atom-package-deps": "^3.0.7",
      "atom-space-pen-views": "^2.0.3",
      "jquery": "^2.1.4"
  },
  "package-deps": [
    "linter",
    "language-jsoniq"
  ],
  "providedServices": {
    "autocomplete.provider": {
        "versions": {
            "2.0.0": "provide"
        }
    },
    "linter": {
        "versions": {
            "1.0.0": "provideLinter"
        }
    }
  },
  "coffeelintConfig": {
    "indentation" : {
        "level" : "error",
        "value" : 4
    },
    "line_endings" : {
        "value" : "unix",
        "level" : "error"
    },
    "max_line_length": {
        "name": "max_line_length",
        "level": "ignore"
    }
  }
}
